{
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "Kudos Generator",
  "description": "Generate kudos for your team",
  "author": "Daniel Junior",
  "action": {
    "default_title": "Kudos Generator",
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*/pull/*"],
      "js": ["contentscript.js"]
    }
  ]
}